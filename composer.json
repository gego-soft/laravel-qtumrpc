{
    "name": "munish/laravel-qtumd",
    "type": "library",
    "description": "Qtum JSON-RPC  for Laravel",
    "keywords": ["laravel","bitcoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/mamunish/laravel-qtumd",
    "license": "MIT",
    "authors": [
        {
            "name": "Munish",
            "email": "munish.ciet.itl22@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "munish/php-bitcoinrpc": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4" : {
            "Munish\\Qtum\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Munish\\Bitcoin\\Providers\\ServiceProvider"
            ]
        }
    }
}
